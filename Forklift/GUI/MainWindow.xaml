<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="Forklift.MainWindow"
        Title="Forklift" Height="350" Width="527" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Closing="WindowClosing">
	<Grid>
		<DataGrid Grid.Column="0" Margin="0,0,0,118" Background="{x:Null}" AutoGenerateColumns="False" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible" BorderBrush="{x:Null}" />
		<GridSplitter Height="5" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0,205,0,0" ResizeBehavior="PreviousAndNext" Grid.Column="1" />
		<TextBox x:Name="OutputTextBox" HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="" Margin="0,210,0,0" d:LayoutOverrides="GridBox" Grid.Column="2" VerticalScrollBarVisibility="Visible" />
	</Grid>
</Window>
